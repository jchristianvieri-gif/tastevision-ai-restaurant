{
  "name": "AI Product Extraction from Images",
  "nodes": [
    {
      "parameters": {
        "path": "/upload-image",
        "methods": ["POST"]
      },
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook"
    },
    {
      "parameters": {
        "operation": "base64ToBinary"
      },
      "name": "Process Image",
      "type": "n8n-nodes-base.base64"
    },
    {
      "parameters": {
        "model": "gpt-4-vision-preview",
        "prompt": "Extract product name, description, and price from this restaurant menu image. Return as JSON: {name: '', description: '', price: 0}"
      },
      "name": "LangChain AI Processing",
      "type": "n8n-nodes-base.openAi"
    },
    {
      "parameters": {
        "operation": "create",
        "table": "products",
        "data": "={{ $json }}"
      },
      "name": "Save to Supabase",
      "type": "n8n-nodes-base.supabase"
    }
  ]
}
